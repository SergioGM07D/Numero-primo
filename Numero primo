int main() {
   int x = 17;
   bool y = true;
   if (x <= 1) {
       y = false;
   }
   else {
       for (int i = 2; i * i <= x ; ++i){
           if (x % i == 0){
               y = false;
               break;
           }
       }
   }
   if (y == true){
       printf ("El número %d es primo", x);
   }
   else{
       printf ("el número %d no es primo", x);
   }
}
